{
  "name": "pnpm-deduplicate",
  "type": "module",
  "version": "0.4.0",
  "description": "Deduplication tool for pnpm-lock.yaml files",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "pnpm-deduplicate": "bin/pnpm-deduplicate.mjs"
  },
  "files": [
    "README.md",
    "dist/*",
    "bin/*"
  ],
  "scripts": {
    "build": "pnpm build:esbuild && pnpm build:tsc",
    "build:esbuild": "node esbuild.mjs",
    "build:tsc": "tsc --build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@pnpm/lockfile-file": "^5.3.2",
    "@pnpm/lockfile-utils": "^4.2.3",
    "@pnpm/lockfile-walker": "^5.0.13",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/manifest-utils": "^3.1.2",
    "@pnpm/read-project-manifest": "^3.0.9",
    "@pnpm/types": "^8.5.0",
    "commander": "^9.4.0",
    "dependency-path": "^9.2.4",
    "path-exists": "^5.0.0",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@ocavue/eslint-config": "^0.4.5",
    "@types/node": "^18.6.3",
    "@types/semver": "^7.3.12",
    "esbuild": "^0.14.53",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.29.0",
    "prettier": "^2.8.0",
    "typescript": "^4.7.4"
  }
}
